{
  "name": "one",
  "version": "1.0.0",
  "description": "React MobX boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development nodemon server/index.js -w server/",
    "start:prod": "NODE_ENV=production node server/server.bundle.js",
    "build:all": "npm run build:client && npm run build:server",
    "build:client": "NODE_ENV=production webpack --config config/webpack.prod.js",
    "build:server": "NODE_ENV=production webpack --config config/webpack.server.js",
    "clean:builds": "rm server/public/main.bundle.js server/public/styles.bundle.css server/public/vendor.bundle.js server/server.bundle.js"
  },
  "keywords": [
    "react",
    "mobx",
    "boilerplate"
  ],
  "author": "Sergey Smirnov",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "json-loader": "0.5.4",
    "nodemon": "^1.9.2",
    "postcss": "^5.0.21",
    "postcss-loader": "^0.9.1",
    "react-hot-loader": "^3.0.0-beta.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "mobx-react-devtools": "^4.2.0",
    "babel-core": "^6.10.4",
    "babel-preset-react": "^6.11.1"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "jade": "^1.11.0",
    "mobx": "2.2.2",
    "mobx-react": "^3.4.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-router": "^2.5.1"
  },
  "babelConfig": {
    "server": {
      "presets": [
        "es2015",
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-decorators-legacy"
      ]
    },
    "client": {
      "presets": [
        "es2015",
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-decorators-legacy"
      ],
      "env": {
        "development": {
          "plugins": [
            "react-hot-loader/babel"
          ]
        }
      }
    }
  }
}
